{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CorsOrigins": [
    "https://localhost:7194",
    "http://localhost:5257"
  ],
  "AllowedHosts": "*",
  "DB_CONN": "",  
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Error",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff}-[{Level:u3}][{UserName}][{Properties}] {Message:lj} {NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Events\\EventLog.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff}-[{Level:u3}][{UserName}][{Properties}] {Message:lj} {NewLine}{Exception}",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 26214400,
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ],
    "Using": [
      "Serilog.Enrichers.ClientInfo"
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithClientIp",
      "WithClientAgent"
    ]
  }
}